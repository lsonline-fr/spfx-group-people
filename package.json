{
  "name": "group-people",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "gulp bundle",
    "clean": "gulp clean",
    "test": "jest",
    "doc": "npm run typedoc -- --options typedoc.js",
    "typedoc": "typedoc"
  },
  "dependencies": {
    "@microsoft/sp-core-library": "1.8.1",
    "@microsoft/sp-lodash-subset": "1.8.1",
    "@microsoft/sp-office-ui-fabric-core": "1.8.0",
    "@microsoft/sp-webpart-base": "1.8.1",
    "@pnp/common": "^1.3.2",
    "@pnp/logging": "^1.3.2",
    "@pnp/odata": "^1.3.2",
    "@pnp/sp": "^1.3.2",
    "@pnp/spfx-controls-react": "^1.12.0",
    "@types/es6-promise": "3.3.0",
    "@types/react": "16.4.2",
    "@types/react-dom": "16.0.5",
    "@types/webpack-env": "1.13.9",
    "office-ui-fabric-react": "5.131.0",
    "react": "16.7.0",
    "react-dom": "16.7.0"
  },
  "resolutions": {
    "@types/react": "16.4.2"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-2.7": "0.4.0",
    "@microsoft/sp-build-web": "1.8.1",
    "@microsoft/sp-module-interfaces": "1.8.1",
    "@microsoft/sp-tslint-rules": "1.8.1",
    "@microsoft/sp-webpart-workbench": "1.8.1",
    "@types/chai": "^4.1.7",
    "@types/enzyme": "3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^16.0.5",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.11",
    "ajv": "^6.10.0",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "gulp": "^3.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.test.+(ts|tsx|js)"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "cobertura"
    ],
    "coverageDirectory": "<rootDir>/jest",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "jest-junit": {
    "output": "./jest/summary-jest-junit.xml"
  }
}
